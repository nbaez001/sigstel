<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Dependencia">
    <select id="Listar" resultType="pe.gob.munihuamanga.sigstel.be.Dependencia">
        select * from dependencia
    </select>
    <select id="Obtener" parameterType="java.lang.Integer" resultType="pe.gob.munihuamanga.sigstel.be.Dependencia">
        select * from dependencia where id=#{id}
    </select>
</mapper>